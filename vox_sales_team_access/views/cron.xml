<odoo>
    <data>

        <record id="ir_cron_users_team_lead" model="ir.cron">
            <field name="name">Lead Users and Teams</field>
            <field name="model_id" ref="crm.model_crm_lead"/>
            <field name="state">code</field>
            <field name="code">model._cron_lead_users_teams()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active">False</field>
        </record>


         <record id="ir_cron_users_team_sale" model="ir.cron">
            <field name="name">Sale Users and Teams</field>
            <field name="model_id" ref="sale.model_sale_order"/>
            <field name="state">code</field>
            <field name="code">model._cron_lead_users_teams()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active">False</field>
        </record>
    </data>
</odoo>