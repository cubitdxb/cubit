<?xml version="1.0" encoding="utf-8"?>
<odoo>

      <record forcecreate="True" id="margin_decimal_price" model="decimal.precision">
            <field name="name">Margin Product Price</field>
            <field name="digits">6</field>
        </record>

       <record model="crm.stage" id="stage_lead5">
        <field name="name">Lead</field>
            <field name="is_lead">True</field>
            <!--            <field name="stages_readonly">True</field>-->
        <field name="sequence">5</field>
    </record>
    <record model="crm.stage" id="stage_lead6">
        <field name="name">Quotation</field>
         <field name="is_opportunity">True</field>
         <!--            <field name="stages_readonly">True</field>-->
        <field name="sequence">6</field>
    </record>

    <record model="crm.stage" id="stage_lead7">
        <field name="name">Quotation Submitted</field>
          <field name="is_import">True</field>
         <!--            <field name="stages_readonly">True</field>-->
        <field name="sequence">7</field>
    </record>


     <record model="crm.stage" id="stage_lead9">
        <field name="name">Negotiation</field>
        <field name="sequence">9</field>
    </record>
     <record model="crm.stage" id="stage_lead10">
        <field name="name">Waiting</field>
        <field name="sequence">10</field>
    </record>

    <record model="crm.stage" id="stage_lead11">
        <field name="name">Won</field>
        <field name="sequence">11</field>
         <!--            <field name="stages_readonly">True</field>-->
          <field name="is_sale_order">True</field>
    </record>

     <record model="crm.stage" id="stage_lead12">
        <field name="name">Lost</field>
        <field name="sequence">12</field>
          <field name="is_lost">True</field>
    </record>

     <record model="crm.stage" id="stage_lead13">
        <field name="name">Onhold</field>
        <field name="sequence">13</field>
    </record>

<!--    <record model="crm.stage" id="stage_lead4">-->
<!--        <field name="name">Won</field>-->
<!--        <field name="fold" eval="False"/>-->
<!--        <field name="is_won">True</field>-->
<!--        <field name="sequence">70</field>-->
<!--    </record>-->
</odoo>